#!/usr/bin/env ruby

require 'lib/users'

Users::initialize
if Users::adduser("ojosa","password") == false
  puts "User exists"
else
  puts "User added"
end

if Users::login("joosa","password") == true
  puts "Success"
else
  puts "Failure"
end
if Users::login("joosa","asdasd") == true
  puts "Success"
else
  puts "Failure"
end

Users::dump
